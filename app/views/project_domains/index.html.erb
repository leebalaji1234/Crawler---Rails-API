<p id="notice"><%= notice %></p>

<h1>Listing Project Domains</h1>

<table>
  <thead>
    <tr>
      <th>Domain</th>
      <th>Project</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_domains.each do |project_domain| %>
      <tr>
        <td><%= project_domain.domain_id %></td>
        <td><%= project_domain.project %></td>
        <td><%= project_domain.user %></td>
        <td><%= link_to 'Show', project_domain %></td>
        <td><%= link_to 'Edit', edit_project_domain_path(project_domain) %></td>
        <td><%= link_to 'Destroy', project_domain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project domain', new_project_domain_path %>
