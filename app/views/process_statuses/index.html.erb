<p id="notice"><%= notice %></p>

<h1>Listing Process Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Process status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @process_statuses.each do |process_status| %>
      <tr>
        <td><%= process_status.process_status %></td>
        <td><%= link_to 'Show', process_status %></td>
        <td><%= link_to 'Edit', edit_process_status_path(process_status) %></td>
        <td><%= link_to 'Destroy', process_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Process status', new_process_status_path %>
