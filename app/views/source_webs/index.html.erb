<p id="notice"><%= notice %></p>

<h1>Listing Source Webs</h1>

<table>
  <thead>
    <tr>
      <th>Url collection name</th>
      <th>Url collection</th>
      <th>Scheduler type</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @source_webs.each do |source_web| %>
      <tr>
        <td><%= source_web.url_collection_name %></td>
        <td><%= source_web.url_collection %></td>
        <td><%= source_web.scheduler_type %></td>
        <td><%= source_web.project %></td>
        <td><%= link_to 'Show', source_web %></td>
        <td><%= link_to 'Edit', edit_source_web_path(source_web) %></td>
        <td><%= link_to 'Destroy', source_web, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Source web', new_source_web_path %>
